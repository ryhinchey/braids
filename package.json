{
  "name": "braids",
  "version": "0.0.1",
  "description": "A tool to get data about a site's performance and network requests",
  "author": "Ryan Hinchey <rlhinchey@gmail.com> (http://twitter.com/ryhinchey)",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": "dist/cli/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "lint": "eslint src/**/*.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "web performance",
    "puppeteer",
    "performance analytics"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern 'src/**/*.*(spec.ts|ts)'"
    }
  },
  "dependencies": {
    "commander": "4.1.0",
    "puppeteer": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/node": "13.1.8",
    "@types/puppeteer": "2.0.0",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "eslint": "6.8.0",
    "husky": "4.2.0",
    "jest": "24.7.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  }
}